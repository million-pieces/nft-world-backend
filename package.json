{
  "name": "million-pieces-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=development node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:up": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:run -f ./src/DAL/ormconfig.ts",
    "migration:mongodb:up": "cross-env NODE_ENV=development nestjs-command migration:up:mongodb",
    "migration:create:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:create -f ./src/DAL/ormconfig.ts -n",
    "migration:generate:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate -f ./src/DAL/ormconfig.ts -n",
    "migration:up:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:run -f ./src/DAL/ormconfig.ts",
    "migration:down:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:revert -f ./src/DAL/ormconfig.ts",
    "migration:create:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:create -f ./src/DAL/ormconfig.ts -n",
    "migration:generate:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:generate -f ./src/DAL/ormconfig.ts -n",
    "migration:up:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:run -f ./src/DAL/ormconfig.ts",
    "migration:down:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:revert -f ./src/DAL/ormconfig.ts",
    "migration:mongodb:up:development": "cross-env NODE_ENV=development nestjs-command migration:up:mongodb",
    "migration:mongodb:up:production": "cross-env NODE_ENV=production nestjs-command migration:up:mongodb",
    "migration:mongodb:down:development": "cross-env NODE_ENV=development nestjs-command migration:down:mongodb",
    "migration:mongodb:down:production": "cross-env NODE_ENV=production nestjs-command migration:down:mongodb",
    "base64toFile:dev": "cross-env NODE_ENV=development nestjs-command base64ToFile",
    "base64toFile:prod": "cross-env NODE_ENV=production nestjs-command base64ToFile",
    "logImagesExtName:dev": "cross-env NODE_ENV=development nestjs-command logImagesExtname",
    "logImagesExtName:prod": "cross-env NODE_ENV=production nestjs-command logImagesExtname",
    "removeNonExistingFiles:dev": "cross-env NODE_ENV=development nestjs-command removeNonExistingFiles",
    "removeNonExistingFiles:prod": "cross-env NODE_ENV=production nestjs-command removeNonExistingFiles",
    "pullNftOwners:dev": "cross-env NODE_ENV=development nestjs-command pullNftOwners",
    "pullNftOwners:prod": "cross-env NODE_ENV=production nestjs-command pullNftOwners"
  },
  "dependencies": {
    "@automapper/classes": "^7.3.14",
    "@automapper/core": "^7.2.1",
    "@automapper/nestjs": "^7.2.1",
    "@automapper/types": "^6.3.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "8.0.3",
    "@types/multer": "^1.4.7",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "excel-coordinates": "^2.0.1",
    "express": "^4.18.0",
    "express-http-context": "^1.2.4",
    "graphql-request": "^4.2.0",
    "http2-wrapper": "^2.1.11",
    "jimp": "^0.15.0",
    "moment": "^2.29.3",
    "mongodb": "^3.7.3",
    "mongoose": "^6.3.1",
    "multer": "1.4.5-lts.1",
    "nestjs-command": "^3.1.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.30.5",
    "ts-node": "^10.0.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "web3": "^1.7.4",
    "web3-utils": "^1.7.3",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.11.31",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "got": "11.8.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
